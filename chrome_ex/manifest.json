{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/fangs.png",
        "48": "images/crow.png",
        "128": "images/pumpkin.png"
      }
    }
  }
  